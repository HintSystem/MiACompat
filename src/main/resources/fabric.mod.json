{
	"schemaVersion": 1,
	"id": "miacompat",
	"version": "${version}",
	"name": "MiACompat",
	"description": "Adds Xaeros minimap / map mod support to the Mine in Abyss minecraft server",
	"authors": ["HintSystem"],
	"contact": {
		"sources": "https://github.com/HintSystem/MiACompat"
	},
	"license": "GNU GPL",
	"icon": "assets/miacompat/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"dev.hintsystem.miacompat.MiACompat"
		],
        "modmenu": [
            "dev.hintsystem.miacompat.mods.SupportModMenu"
        ]
	},
	"mixins": [
		"miacompat.mixins.json"
	],
	"depends": {
		"java": ">=21",
		"minecraft": "1.21.11",
		"fabricloader": ">=0.18.0",
		"fabric-api": "*",
        "yet_another_config_lib_v3": "*"
	},
    "recommends": {
        "xaerominimap": "*",
        "xaeroworldmap": "*"
    },
    "suggests": {
        "pingwheel": "~1.12.0"
    }
}